class Solution {
    public int maxWidthRamp(int[] nums) {
        int maxi=Integer.MIN_VALUE;
        for(int i=0;i<nums.length;i++){
            int k=i;
            for(int j=i+1;j<nums.length;j++){
                if(nums[i]<=nums[j])
                    k=j;
            }
            maxi=Math.max(maxi,k-i);
        }
        return maxi;
    }
}
